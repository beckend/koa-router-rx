{
  "name": "koa-router-rx",
  "version": "0.3.2",
  "description": "Reactive router middleware for koa",
  "main": "dist/index.js",
  "repository": "https://github.com/mfellner/koa-router-rx.git",
  "author": "Maximilian Fellner <max.fellner@gmail.com>",
  "license": "MIT",
  "keywords": [
    "route",
    "router",
    "middleware",
    "koa",
    "Observable",
    "Observables",
    "ReactiveExtensions",
    "ReactiveX",
    "RxJS",
    "Rx"
  ],
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "dist": "babel -d dist lib",
    "start": "babel-node example/server.js",
    "test": "NODE_ENV=test jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "flow-check": "flow check",
    "flow-copy-files": "cpy lib/*.flow dist",
    "flow-gen-files": "flow-copy-source -v lib dist && cpy lib/*.flow dist",
    "flow-typed-install": "flow-typed install -o",
    "prepublish": "npm run clean && npm run dist && npm run flow-gen-files"
  },
  "dependencies": {
    "koa-router": "7.1.0",
    "rxjs": "5.0.3"
  },
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-jest": "18.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-syntax-flow": "6.18.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-preset-latest-minimal": "1.1.2",
    "cpy-cli": "1.0.1",
    "flow-bin": "0.38.0",
    "flow-copy-source": "1.1.0",
    "flow-typed": "2.0.0",
    "jest": "18.1.0",
    "koa": "2.0.0",
    "rimraf": "2.5.4",
    "supertest": "3.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageReporters": [
      "json",
      "lcov",
      "html",
      "text"
    ]
  }
}
